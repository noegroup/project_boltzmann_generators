	Day = {16},
	Issn = {0027-8424},
	Journal = {Proc. Natl. Acad. Sci. USA},
	Month = mar,
	Number = {6},
	Pages = {2782--2787},
	Pmid = {10077588},
	Posted-At = {2009-02-19 05:40:40},
	Priority = {2},
	Title = {{Multiple pathways on a protein-folding energy landscape: kinetic evidence.}},
