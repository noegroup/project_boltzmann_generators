	Journal = {Proceedings of the National Academy of Sciences of the United States of America},
	Month = mar,
	Number = {12},
	Pages = {4907--4912},
	Pmid = {17360387},
	Posted-At = {2009-02-19 05:23:28},
	Priority = {2},
	Title = {{Predicting coupling limits from an experimentally determined energy landscape}},
	Url = {http://dx.doi.org/10.1073/pnas.0608756104},
