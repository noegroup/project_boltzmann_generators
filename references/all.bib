	Number = {3},
	Pages = {929--937},
	Posted-At = {2009-04-08 15:25:16},
	Priority = {2},
	Title = {{Transition States in Protein Folding Kinetics: Modeling Phi-Values of Small Beta-Sheet Proteins}},
	Url = {http://www.cell.com/biophysj/abstract/S0006-3495(08)70691-X},
	Volume = {94},
	Year = {2008},
	Bdsk-Url-1 = {http://www.cell.com/biophysj/abstract/S0006-3495(08)70691-X}}

